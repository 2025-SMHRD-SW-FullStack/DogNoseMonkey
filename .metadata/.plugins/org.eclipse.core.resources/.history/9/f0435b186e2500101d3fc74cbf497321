package Inheritance;

public class Question {

	public static void main(String[] args) {
		System.out.println("===== 싱글벙글 상속 퀴즈 =====");
		try {
			Thread.sleep(5000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println("문제를 맞추고 상속킹이 되자!");
		System.out.println("문제를 많이 맞추신 분에게는 추첨을 통해 축하해드리니");
		System.out.println("문제를 많이 맞추고 축하를 받아봅시다!");
		try {
			Thread.sleep(5000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(".　∩＿＿＿∩\r\n"
				+ "　 | ノ　 ノヽ、ヽ\r\n"
				+ "　/　　●　　　● |\r\n"
				+ " |　 　　　( _●_)　ミ　　으... 전 안할래요...\r\n"
				+ "彡､　　/⌒)(⌒ヽ/\r\n"
				+ " ./　　/　 /　＼　 ヽ\r\n"
				+ " l　　　 ノ　　　 `ｰ‐'\r\n"
				+ "");
		try {
			Thread.sleep(3000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println("\r\n"
				+ "\r\n"
				+ "                                                  \r\n"
				+ "                    ..                            \r\n"
