				+ "          $=@ ,#@@@@@@@    $@@@ @@@#$    #@ @     \r\n"
				+ "    ,*!!!!!!!$  !     @     @     !  @=*@ @ @     \r\n"
				+ "    --,,,,,,,!  $ ==@ @     #@@@@ !       @ @     \r\n"
				+ "     ,-------,  * ~;; @         ; !       @$@     \r\n"
				+ "                !-----@         :-=               \r\n"
				+ "                 .....           .                \r\n"
				+ "                        ................     .    \r\n"
				+ "                      ..,,,,,..,,..,.......       \r\n"
				+ "                       -~,,,,,..,,,,,...,,.   .   \r\n"
				+ "                      .:~-,,,,-----,,,,,--,...    \r\n"
				+ "                     .,!~~~:!-~;------,,,,,,-- .  \r\n"
				+ "                     .,*~~::$#;~.~-,;,~!--,-~~    \r\n"
				+ "                    .,:!:-,,;!:,. ,.~=@*;~--~:. . \r\n"
				+ "                    ,~;*:,,,~:-,,,,,-:;~--,-:!.   \r\n"
				+ "                     :;!:-,,,,...  ..,--,,,-;!. . \r\n"
				+ "                     :;;~-,,,...--.. ..,,--~;;,.  \r\n"
				+ "                     ~~~----,..!*;:- ..,,,,:::,.  \r\n"
				+ "                     ~-~~---,,,*#@*~...-------.   \r\n"
				+ "    ,                ---~-~---~!#@*~-.,----,...   \r\n"
				+ "   -,..              ~----~-::;!!!!~--:----,  .   \r\n"
				+ "  .,,..             .-------;**!!*!!:!~-~~,.  .   \r\n"
				+ "  ,,,...           .,-------:;;:::~::~~~-.. ...   \r\n"
				+ "  ,,,....         ..,------,-;!::::::~~-,.. ..,   \r\n"
				+ "  ,-,,. ......  ...,,,,,-----~;;::::~~-,...   .   \r\n"
				+ "  ~~~,...,.......,,,,---------~:::~~~-,,,..       \r\n"
				+ "  ~~:~,,--,..,.,.,,,----------~~~~~~-,,,...       \r\n"
				+ "   :::---,,,,,,,,,----~--------~~~~-,.....        \r\n"
				+ "   ,~;~~~---,,,,-,---~~-----------,,,,.. .     .  \r\n"
				+ "      ~~~~~~~-,------~~-------------,...,..       \r\n"
